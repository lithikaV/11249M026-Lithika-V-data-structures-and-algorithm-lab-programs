#include <stdio.h>
#define SIZE 5

int stack[SIZE];
int top = -1;

// Function to push an element
void push() {
    int value;
    if (top == SIZE - 1) {
        printf("Stack Overflow! Cannot visit more pages.\n");
    } else {
        printf("Enter page number to visit: ");
        scanf("%d", &value);
        top++;
        stack[top] = value;
        printf("Page %d visited successfully.\n", value);
    }
}

// Function to pop an element
void pop() {
    if (top == -1) {
        printf("Stack Underflow! No pages to go back to.\n");
    } else {
        printf("Going back from page %d.\n", stack[top]);
        top--;
    }
}

// Function to display stack elements
void display() {
    if (top == -1) {
        printf("No browsing history.\n");
    } else {
        printf("Current browsing history:\n");
        for (int i = top; i >= 0; i--) {
            printf("Page %d\n", stack[i]);
        }
    }
}

// Main function
int main() {
    int choice;
    printf("=== Browser History System (Stack using Array) ===\n");

    while (1) {
        printf("\n1. Visit New Page (Push)\n");
        printf("2. Go Back (Pop)\n");
        printf("3. View History (Display)\n");
        printf("4. Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1: push(); break;
            case 2: pop(); break;
            case 3: display(); break;
            case 4: return 0;
            default: printf("Invalid choice! Please try again.\n");
        }
    }
}
